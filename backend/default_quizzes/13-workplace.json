{
  "content": {
    "quiz": {
      "title": "Évaluation de la santé mentale au travail",
      "description": "Ce test a été conçu pour vous aider à évaluer différents aspects de votre santé mentale liés à votre environnement professionnel. Il vise à fournir une évaluation rapide de votre bien-être psychologique dans le contexte du travail.",
      "image_url": "https://www.efficience-santeautravail.org/wp-content/uploads/2024/10/sante-mentale-2024-2.png",
      "quiz_type_id": 2
    },
    "quiz_type": {
      "id": 2,
      "name": "Santé mentale"
    },
    "questions": [
      {
        "text": "Dans quelle mesure la charge de travail vous semble-t-elle gérable ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Très gérable",
          "max_label": "Très accablante"
        },
        "required": true,
        "order": 1,
        "image_url": ""
      },
      {
        "text": "À quel point êtes-vous stressé(e) par votre travail ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Pas du tout stressé(e)",
          "max_label": "Extrêmement stressé(e)"
        },
        "required": true,
        "order": 2,
        "image_url": ""
      },
      {
        "text": "Dans quelle mesure êtes-vous satisfait(e) de votre équilibre entre le travail et votre vie personnelle ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Très satisfait(e)",
          "max_label": "Très insatisfait(e)"
        },
        "required": true,
        "order": 3,
        "image_url": ""
      },
      {
        "text": "À quel point vous sentez-vous capable de détacher votre esprit du travail pendant vos moments de repos ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Très capable",
          "max_label": "Pas du tout capable"
        },
        "required": true,
        "order": 4,
        "image_url": ""
      },
      {
        "text": "Dans quelle mesure vous sentez-vous soutenu(e) par vos collègues et votre superviseur ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Très soutenu(e)",
          "max_label": "Pas du tout soutenu(e)"
        },
        "required": true,
        "order": 5,
        "image_url": ""
      },
      {
        "text": "À quel point vous sentez-vous isolé(e) ou exclu(e) au travail ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Pas du tout isolé(e)",
          "max_label": "Très isolé(e)"
        },
        "required": true,
        "order": 6,
        "image_url": ""
      },
      {
        "text": "Dans quelle mesure vous sentez-vous autorisé(e) à prendre des décisions dans votre travail ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Très autorisé(e)",
          "max_label": "Pas du tout autorisé(e)"
        },
        "required": true,
        "order": 7,
        "image_url": ""
      },
      {
        "text": "À quel point vous sentez-vous contrôlé(e) par votre travail ou votre environnement professionnel ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Pas du tout contrôlé(e)",
          "max_label": "Très contrôlé(e)"
        },
        "required": true,
        "order": 8,
        "image_url": ""
      },
      {
        "text": "Dans quelle mesure vous sentez-vous valorisé(e) pour votre travail ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Très valorisé(e)",
          "max_label": "Pas du tout valorisé(e)"
        },
        "required": true,
        "order": 9,
        "image_url": ""
      },
      {
        "text": "À quel point vous sentez-vous récompensé(e) pour vos efforts et vos réalisations professionnelles ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Très récompensé(e)",
          "max_label": "Pas du tout récompensé(e)"
        },
        "required": true,
        "order": 10,
        "image_url": ""
      },
      {
        "text": "Dans quelle mesure vous sentez-vous émotionnellement épuisé(e) à cause de votre travail ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Pas du tout épuisé(e)",
          "max_label": "Très épuisé(e)"
        },
        "required": true,
        "order": 11,
        "image_url": ""
      },
      {
        "text": "À quel point vous sentez-vous capable de gérer vos émotions au travail ?",
        "question_type": "linear_scale",
        "options": {
          "min_value": 1,
          "max_value": 6,
          "min_label": "Très capable",
          "max_label": "Pas du tout capable"
        },
        "required": true,
        "order": 12,
        "image_url": ""
      }
    ],
    "dimensions": [
      {
        "name": "Santé mentale au travail",
        "description": "Évaluation globale du bien-être psychologique dans l'environnement professionnel",
        "order": 1
